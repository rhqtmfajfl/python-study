    "regex = '\\(.*\\)|\\s-\\s.*' # 괄호안의 문장 제거에 사용 될 것\n",
    "regex1_1 = '\\（.*\\）|\\s-\\s.*' # 괄호가 전각 형식으로 된것 제거에 사용 될 것\n",
    "regex1_2 = '\\('              # 괄호가 앞에 한개 있는 것 제거에 사용 될 것\n",
    "regex2 ='\\[.*\\]|\\s-\\s.*'     # 대괄호 제거에 사용 될 것\n",
    "regex2_1 = '\\［.*\\］|\\s-\\s.*'# 전각 형식의 대괄호 제거에 사용 될 것\n",
    "regex2_2 = '\\['              # 대괄호 한개 있는 것 제거에 사용 될 것\n",
    "regex3 = '\\{.*\\}|\\s-\\s.*'    # 중괄호 제거에 사용 될 것\n",
    "regex4 = '\\d+\\.'             # 숫자 + . 제거에 사용 될 것\n",
    "regex5 = '\\♦ |\\：|\\；|\\，|\\、|\\•|\\¿|\\♪|\\*|\\#|\\=|\\::Wh'  # 각 특수문자 제거 +전각문자 \n",
    "regex6 = '\\/+\\d{2}'                                       # / + 숫자 제거에 사용 될 것\n",
    "regex7 = '\\D+\\/'                                          # 문자 + / 제거에 사용 될 것\n",
    "pattern = '(http|ftp|https)//(?:[-\\w.]|(?:\\da-fA-F]{2}))+'  # url 제거에 사용 될 것\n",
    "\n",
    "\n",
    "test_regex =':Wh\\:' # 세미콜론 제거에 사용 될 것\n",
    "test_regex2 = '\\:+\\D'# 세미콜론 제거에 사용 될 것\n",
    "test_regex3 = '\\D+\\:'# 세미콜론 제거에 사용 될 것\n",
    "test_regex4 = '-' # 하이픈 제거에 사용될 것\n",
    "test_regex5 = '-'# 하이픈 제거에 사용될 것\n",
