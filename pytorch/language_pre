   "source": [
    "regex = '\\(.*\\)|\\s-\\s.*'\n",
    "regex1_1 = '\\（.*\\）|\\s-\\s.*'\n",
    "regex1_2 = '\\('\n",
    "regex2 ='\\[.*\\]|\\s-\\s.*'\n",
    "regex2_1 = '\\［.*\\］|\\s-\\s.*'\n",
    "regex2_2 = '\\['\n",
    "regex3 = '\\{.*\\}|\\s-\\s.*'\n",
    "regex4 = '\\d+\\.'\n",
    "regex5 = '\\♦ |\\：|\\；|\\，|\\、|\\•|\\¿|\\♪|\\*|\\#|\\=|\\—|\\::Wh'\n",
    "regex6 = '\\/+\\d{2}'\n",
    "regex7 = '\\D+\\/'\n",
    "pattern = '(http|ftp|https)//(?:[-\\w.]|(?:\\da-fA-F]{2}))+'  # url 제거\n",
    "\n",
    "test_regex =':Wh\\:'\n",
    "test_regex2 = '\\:+\\D'\n",
    "test_regex3 = '\\D+\\:'\n",
    "# text = re.sub(pattern=pattern,repl=' ',string=text)"
   ]
  },
