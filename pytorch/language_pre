    "\n",
    "half = ':'\n",
    "\n",
    "diff = '0xfee0'\n",
    "blank = '0x3000'\n",
    "\n",
    "#16진수인 ascii code\n",
    "# hex_ascii_full = hex(ord(full))\n",
    "# hex_ascii_half = ord(half)\n",
    "hex_ascii_full = ord(full)\n",
    "hex_ascii_half = ord(half)\n",
    "hex_ascii_diff = int(diff,16)\n",
    "hex_ascii_blank = int(blank, 16)\n",
    "\n",
    "#16진수 형태의 string\n",
    "hex_half = hex(hex_ascii_half)\n",
    "hex_blank = hex(hex_ascii_blank)"
   ]
