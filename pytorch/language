    "import urllib.request\n",
    "from tqdm.notebook import tqdm\n",
    "\n",
    "file_len = []\n",
    "\n",
    "path = 'C:/Users/CPB06GameN/Downloads/opus-100'\n",
    "file_list = os.listdir(path)\n",
    "file_list_py = [file for file in file_list if len(file) <= 3]\n",
    "\n",
    "a=1\n",
    "# file_sorted = sorted(file_list_py)\n",
    "\n",
    "# print(file_list_py)\n",
    "src_lang = pd.DataFrame(columns=['src-lang'])\n",
    "tgt_lang = pd.DataFrame(columns=['tgt-lang'])\n",
    "final = pd.DataFrame(columns=['src-lang','src', 'tgt','tgt-lang'])\n",
    "for file in tqdm(file_list_py):\n",
    "    f = open(path + '/' + file + '/opus.en-{}-dev.{}'.format(file, file), 'r' ,encoding='UTF8')\n",
    "    f2 = open(path + '/' + file + '/opus.en-{}-dev.en'.format(file), 'r',encoding='UTF8')\n",
    "    \n",
    "    lines = f.readlines()\n",
    "    lines2 = f2.readlines()\n",
    "    \n",
    "    test_split = '\\n'.join(lines)\n",
    "    test_split2 = '\\n'.join(lines2)\n",
    "\n",
    "    test_split_2 =test_split.split('\\n')\n",
    "    test_split_2_2 =test_split2.split('\\n')\n",
    "\n",
    "    \n",
    "    test_drop = list(filter(None, test_split_2))\n",
    "    test_drop_2 = list(filter(None, test_split_2_2))\n",
    "\n",
    "    \n",
    "    df = pd.DataFrame(test_drop, columns =['src'])\n",
    "    df_2 = pd.DataFrame(test_drop_2, columns=['tgt'])\n",
    "    \n",
    "    test_combine = pd.concat([src_lang, df])\n",
    "    test_combine_2 = pd.concat([df_2, tgt_lang])\n",
    "    \n",
    "    test_combine_result = test_combine.replace(np.nan, '{}'.format(file))\n",
    "    test_combine_result_2 = test_combine_2.replace(np.nan, '{}'.format('en'))\n",
    "    \n",
    "    temp = pd.concat([test_combine_result, test_combine_result_2], axis=1)\n",
    "    tmp = temp.sample(200)\n",
    "    final = pd.concat([final, tmp])\n",
    "#     test_combine_result_test\n",
    "#     print(test_combine_result_2)\n",
    "    print(final)\n",
